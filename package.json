{
  "name": "TBD",
  "version": "1.0.0",
  "description": "TBD",
  "author": "TBD",
  "license": "MIT",
  "main": "server.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TBD"
  },
  "homepage": "TBD",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "seed": "node scripts/seedDB.js",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "nodemonConfig": {
    "events": {
      "restart": "npm kill",
      "crash": "npm kill"
    },
    "delay": "3000",
    "ignore": [
      "client/*"
    ]
  },
  "nyc": {
    "exclude": [
      "client"
    ]
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "concurrently": "^5.1.0",
    "coveralls": "^3.0.9",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.20.0",
    "kill-port": "^1.6.0",
    "mocha": "^7.0.1",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^2.0.2",
    "nyc": "^15.0.0",
    "sequelize-cli": "^5.5.1",
    "supertest": "^4.0.2",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "@amcharts/amcharts4": "^4.9.1",
    "axios": "^0.19.2",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "helmet": "^3.21.2",
    "if-env": "^1.0.4",
    "moment": "^2.24.0",
    "mongojs": "^3.1.0",
    "mongoose": "^5.8.11",
    "morgan": "^1.9.1",
    "mysql2": "^2.1.0",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "passport-session": "^1.0.2",
    "react": "^16.12.0",
    "react-big-calendar": "^0.24.0",
    "react-countdown-circle-timer": "^1.1.1",
    "react-dom": "^16.12.0",
    "react-google-charts": "^3.0.15",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^3.3.1",
    "render": "^0.1.4",
    "sequelize": "^4.44.3",
    "yarn": "^1.21.1"
  }
}
